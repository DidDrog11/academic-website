	note = {Publisher: Cold Spring Harbor Laboratory Press; http://web.archive.org/web/20200825124007/https://www.medrxiv.org/content/10.1101/2020.07.15.20150490v1},
	pages = {2020.07.15.20150490},
	file = {Full Text PDF:C\:\\Users\\ucbtds4\\Zotero\\storage\\V3HFRX6E\\Hoertel et al. - 2020 - Observational Study of Haloperidol in Hospitalized.pdf:application/pdf;Snapshot:C\:\\Users\\ucbtds4\\Zotero\\storage\\FN8VMELS\\2020.07.15.html:text/html}
