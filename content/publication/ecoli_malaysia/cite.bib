	file = {Snapshot:C\:\\Users\\ucbtds4\\Zotero\\storage\\C4QSXBE2\\1733.html:text/html;Full Text PDF:C\:\\Users\\ucbtds4\\Zotero\\storage\\DCXCWH9Z\\Borobia et al. - 2020 - A Cohort of Patients with COVID-19 in a Major Teac.pdf:application/pdf}
