	file = {Springer Full Text PDF:C\:\\Users\\ucbtds4\\Zotero\\storage\\XHVN2RAD\\Goüy de Bellocq and Leirs - 2010 - Polymorphism and signatures of selection in the mu.pdf:application/pdf}
}

