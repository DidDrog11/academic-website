	number = {493},
	urldate = {2020-12-18},
	journal = {Journal of the American Statistical Association},
	author = {Mohler, G. O. and Short, M. B. and Brantingham, P. J. and Schoenberg, F. P. and Tita, G. E.},
	month = mar,
	year = {2011},
	pages = {100--108},
	file = {Mohler et al. - 2011 - Self-Exciting Point Process Modeling of Crime.pdf:C\:\\Users\\ucbtds4\\Zotero\\storage\\TMYGLYKQ\\Mohler et al. - 2011 - Self-Exciting Point Process Modeling of Crime.pdf:application/pdf}
}

