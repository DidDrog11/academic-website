	urldate = {2020-07-21},
	journal = {medRxiv},
	author = {Yanover, Chen and Mizrahi, Barak and Kalkstein, Nir and Marcus, Karni and Akiva, Pinchas and Barer, Yael and Shalev, Varda and Chodick, Gabriel},
