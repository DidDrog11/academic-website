	file = {Snapshot:C\:\\Users\\ucbtds4\\Zotero\\storage\\KHQ753LV\\2020.05.18.html:text/html;Full Text PDF:C\:\\Users\\ucbtds4\\Zotero\\storage\\XA5ZSZZD\\Kim et al. - 2020 - Interim Analysis of Risk Factors for Severe Outcom.pdf:application/pdf}
