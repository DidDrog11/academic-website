	pmid = {28875769},
	pmcid = {PMC5694855},
	keywords = {Animals, Humans, agriculture, Zoonoses, Prevalence, zoonotic disease, West Africa, Lassa fever, Incidence, Lassa Fever, Mastomys natalensis, Topography, Medical, Africa, Western, arenavirus, One Health, viral haemorrhagic disease, *Lassa Fever/ep [Epidemiology], Africa, Western/ep [Epidemiology], *Zoonoses/ep [Epidemiology]},
	pages = {276--288}
}

@article{gouy_de_bellocq_polymorphism_2010,
	title = {Polymorphism and signatures of selection in the multimammate rat {DQB} gene},
