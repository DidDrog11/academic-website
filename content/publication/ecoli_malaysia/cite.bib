	pages = {1392--1400},
	file = {PubMed Central Full Text PDF:C\:\\Users\\ucbtds4\\Zotero\\storage\\NC5Y4GXJ\\Polubriaginof et al. - 2018 - Challenges with Collecting Smoking Status in Elect.pdf:application/pdf}
