	note = {Publisher: Cold Spring Harbor Laboratory Press; http://web.archive.org/web/20200826122630/https://www.medrxiv.org/content/10.1101/2020.07.21.20153650v1},
	pages = {2020.07.21.20153650},
	file = {Full Text PDF:C\:\\Users\\ucbtds4\\Zotero\\storage\\JP44UF4H\\Thompson et al. - 2020 - Patient characteristics and predictors of mortalit.pdf:application/pdf;Snapshot:C\:\\Users\\ucbtds4\\Zotero\\storage\\WWSMRL3E\\2020.07.21.html:text/html}
