	note = {\_eprint: https://onlinelibrary.wiley.com/doi/pdf/10.1111/j.1749-4877.2007.00065.x},
	keywords = {survival, Mastomys, capture-recapture, maturation, multistate model},
	pages = {220--232},
	file = {Full Text PDF:C\:\\Users\\ucbtds4\\Zotero\\storage\\QZM9ZVYL\\Sluydts et al. - 2007 - Survival and maturation rates of the African roden.pdf:application/pdf;Snapshot:C\:\\Users\\ucbtds4\\Zotero\\storage\\UW2H6UEL\\j.1749-4877.2007.00065.html:text/html}
