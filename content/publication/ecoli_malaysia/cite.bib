@article{mehand_who_2018,
	title = {The {WHO} {R}\&{D} {Blueprint}: 2018 review of emerging infectious diseases requiring urgent research and development efforts},
	volume = {159},
	issn = {0166-3542},
	shorttitle = {The {WHO} {R}\&{D} {Blueprint}},
	url = {http://www.sciencedirect.com/science/article/pii/S0166354218305643},
	doi = {10.1016/j.antiviral.2018.09.009},
	abstract = {The Research and Development (R\&D) Blueprint is a World Health Organization initiative to reduce the time between the declaration of a public health emergency and the availability of effective diagnostic tests, vaccines, and treatments that can save lives and avert a public health crisis. The scope of the Blueprint extends to severe emerging diseases for which there are insufficient or no presently existing medical countermeasures or pipelines to produce them. In February 2018, WHO held an informal expert consultation to review and update the list of priority diseases, employing a prioritization methodology which uses the Delphi technique, questionnaires, multi-criteria decision analysis, and expert review to identify relevant diseases. The committee determined that, given their potential to cause a public health emergency and the absence of efficacious drugs and/or vaccines, there is an urgent need for accelerated R\&D for (in no order of priority) Crimean-Congo haemorrhagic fever, Ebola virus and Marburg virus disease, Lassa fever, Middle East Respiratory Syndrome (MERS) and Severe Acute Respiratory Syndrome (SARS), Nipah and henipaviral diseases, Rift Valley fever and Zika virus disease. The experts also included “Disease X,” representing the awareness that a previously unknown pathogen could cause a major public health emergency. This report describes the methods and results of the 2018 prioritization review.},
	language = {en},
	urldate = {2020-10-21},
	journal = {Antiviral Research},
	author = {Mehand, Massinissa Si and Al-Shorbaji, Farah and Millett, Piers and Murgue, Bernadette},
	month = nov,
	year = {2018},
	keywords = {Experts' opinions, Multi-criteria decision analysis, Multidisciplinary method, Prioritization, Severe emerging infectious diseases},
	pages = {63--67},
	file = {ScienceDirect Full Text PDF:C\:\\Users\\ucbtds4\\Zotero\\storage\\FWFHVL44\\Mehand et al. - 2018 - The WHO R&D Blueprint 2018 review of emerging inf.pdf:application/pdf}
}

@article{ter_meulen_hunting_1996,
	title = {Hunting of {Peridomestic} {Rodents} and {Consumption} of {Their} {Meat} as {Possible} {Risk} {Factors} for {Rodent}-to-{Human} {Transmission} of {Lassa} {Virus} in the {Republic} of {Guinea}},
	volume = {55},
	issn = {0002-9637, 1476-1645},
	url = {10.4269/ajtmh.1996.55.661},
	doi = {10.4269/ajtmh.1996.55.661},
	abstract = {Abstract In this population-based study, we correlated possible risk factors for rodent-to-human transmission of Lassa virus with markers of Lassa fever in two different regions of the Republic of Guinea (Prefectures of Pita and Gueckedou). Antibody prevalence was 2.6\% (6 of 232) in Pita compared with 14.0\% (105 of 751) in Gueckedou, with up to 35.0\% seropositivity in selected villages of the higher prevalence area. We observed three major risk factors in Gueckedou favoring Lassa virus transmission: rodent infestation was much higher, food was more often stored uncovered and most strikingly, peridomestic rodents were hunted as a protein source by 91.5\% of the population as opposed to 0\% in Pita. To control for the confounding effects of differences in rodent infestation and food storage, rodent consumption was analyzed as a risk factor for transmission of Lassa virus comparing rodent consumers (RC) and nonconsumers (NC) in Gueckedou only: 14.6\% of RC had Lassa virus antibodies versus 7.4\% of NC (P = 0.1) and 23.0\% of RC reported a history of a febrile illness with hearing loss (the most common sequel of Lassa fever) versus 6.1\% of NC (P = 0.003).},
	language = {en},
	number = {6},
	urldate = {2020-10-21},
	journal = {The American Journal of Tropical Medicine and Hygiene},
	author = {Ter Meulen, J. and Lukashevich, I. and Sidibe, K. and Inapogui, A. and Marx, M. and Dorlemann, A. and Yansane, M. L. and Koulemou, K. and Chang-Claude, J. and Schmitz, H.},
	month = dec,
	year = {1996},
	note = {Publisher: The American Society of Tropical Medicine and Hygiene},
	keywords = {Meat, Animals, Humans, Rodentia, Prevalence, Lassa virus, Risk Factors, Adolescent, Adult, Age Factors, Antibodies, Viral, Female, Guinea, Lassa Fever, Male, Middle Aged, Child, Child, Preschool, Multivariate Analysis, Aged, Aged, 80 and over, Muridae, Analysis of Variance, Food Microbiology},
