@article{bonwitt_at_2017,
	title = {At {Home} with {Mastomys} and {Rattus}: {Human}-{Rodent} {Interactions} and {Potential} for {Primary} {Transmission} of {Lassa} {Virus} in {Domestic} {Spaces}},
	volume = {96},
	issn = {1476-1645},
	shorttitle = {At {Home} with {Mastomys} and {Rattus}},
	url = {10.4269/ajtmh.16-0675},
	doi = {10.4269/ajtmh.16-0675},
	abstract = {AbstractThe multimammate mouse (Mastomys natalensis) is the reservoir for Lassa virus (LASV). Zoonotic transmission occurs when humans are directly or indirectly exposed to fluids of the multimammate mouse, such as urine, saliva, and blood. Housing characteristics and domestic organization affect rodent density in and around households and villages, and are likely to be a risk factor for Lassa fever in humans where the reservoir exists. We use semi-structured interviews (N = 51), a quantitative survey (N = 429), direct observations, and a rodent ecology study to provide new insights into how the organization of domestic spaces brings together humans and rodents and creates pathways for infection in rural settlements in Bo District, Sierra Leone. Rodents were frequently reported inside houses (92.4\% of respondents), in which we predominantly trapped M. natalensis (57\% of trapped rodents) and Rattus rattus (38\% of trapped rodents). Building design and materials provide hiding and nesting places for rodents and lead to close proximity with humans. Patterns of contact are both unintentional and intentional and research participants reported high levels of contact with rodents (34.2\% of respondents) and rodent fluids (52.8\% of respondents). Rodents are also perceived as a serious threat to food security. These results present detailed knowledge about how humans live with and come into contact with rodents, including the LASV reservoir. Our results argue for further collaborative research in housing and environmental modification such as ceiling construction, food storage, and sanitation as prevention against zoonotic LASV transmission.},
	language = {eng},
	number = {4},
	journal = {The American Journal of Tropical Medicine and Hygiene},
	author = {Bonwitt, Jesse and Sáez, Almudena Mari and Lamin, Joseph and Ansumana, Rashid and Dawson, Michael and Buanie, Jacob and Lamin, Joyce and Sondufu, Diana and Borchert, Matthias and Sahr, Foday and Fichet-Calvet, Elisabeth and Brown, Hannah},
	month = apr,
	year = {2017},
	pmid = {28167603},
	pmcid = {PMC5392645},
	keywords = {Animals, Humans, Rats, Disease Reservoirs, Housing, Lassa virus, Adolescent, Adult, Female, Lassa Fever, Male, Murinae, Sierra Leone, Child, Child, Preschool, Food Supply, Rodent Control, Young Adult},
	pages = {935--943},
	file = {Full Text:C\:\\Users\\ucbtds4\\Zotero\\storage\\NKEP3HCR\\Bonwitt et al. - 2017 - At Home with Mastomys and Rattus Human-Rodent Int.pdf:application/pdf}
}

