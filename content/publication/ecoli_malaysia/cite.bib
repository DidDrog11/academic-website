	file = {Full Text PDF:C\:\\Users\\ucbtds4\\Zotero\\storage\\VUH72LXS\\Yan et al. - 2020 - Role of Drugs used for chronic disease management .pdf:application/pdf;Snapshot:C\:\\Users\\ucbtds4\\Zotero\\storage\\KPPNYMBP\\2020.04.24.html:text/html;Full Text PDF:C\:\\Users\\ucbtds4\\Zotero\\storage\\28L6IGYB\\Yan et al. - 2020 - Role of Drugs used for chronic disease management .pdf:application/pdf;Snapshot:C\:\\Users\\ucbtds4\\Zotero\\storage\\CMN4JEJG\\2020.04.24.html:text/html}
