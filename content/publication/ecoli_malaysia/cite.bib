	author = {Miyara, Makoto and Tubach, Florence and Martinez, Valerie and Morelot-Panzini, Capucine and Pernet, Julie and Haroche, Julien and Lebbah, Said and Morawiec, Elise and Gorochov, Guy and Caumes, Eric and Hausfater, Pierre and Combes, Alain and Similowski, Thomas and Amoura, Zahir},
