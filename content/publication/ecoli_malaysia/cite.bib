	pages = {e37068},
	file = {Full Text PDF:C\:\\Users\\ucbtds4\\Zotero\\storage\\LHNJRRJX\\Lalis et al. - 2012 - The Impact of Human Conflict on the Genetics of Ma.pdf:application/pdf;Snapshot:C\:\\Users\\ucbtds4\\Zotero\\storage\\KPZW4D3R\\article.html:text/html;Full Text PDF:C\:\\Users\\ucbtds4\\Zotero\\storage\\BYH4B9DR\\Lalis et al. - 2012 - The Impact of Human Conflict on the Genetics of Ma.pdf:application/pdf;Snapshot:C\:\\Users\\ucbtds4\\Zotero\\storage\\79F2TEYE\\article.html:text/html}
