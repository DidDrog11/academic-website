@article{savarraj_three-month_nodate,
	title = {Three-month outcomes in hospitalized {COVID}-19 patients},
	abstract = {COVID-19 is an ongoing pandemic with a devastating impact on public health. Acute neurological symptoms have been reported after a COVID-19 diagnosis, however there is no data available on the long-term neurological symptoms. Using a prospective registry of hospitalized COVID-19 patients, we assessed the neurological assessments (including functional, cognitive and psychiatric assessments) of several hospitalized patients at 3 months. Our main finding is that 71\% of the patients still experienced neurological symptoms at 3 months and the most common symptoms being fatigue (42\%) and PTSD (29\%). 64\% of the patients report pain symptoms we well. Cognitive symptoms were found in 12\%. Our preliminary findings suggests the importance of investigating long-term and rationalizes the need for further studies investigating the neurologic outcomes after COVID-19.},
