@article{suzan_modeling_2006,
	title = {{MODELING} {HANTAVIRUS} {RESERVOIR} {SPECIES} {DOMINANCE} {IN} {HIGH} {SEROPREVALENCE} {AREAS} {ON} {THE} {AZUERO} {PENINSULA} {OF} {PANAMA}},
