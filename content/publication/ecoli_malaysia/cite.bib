@article{kangbai_lassa_2019,
	title = {Lassa {Fever} in post-{Ebola} {Sierra} {Leone}. {Sociodemographics} and case fatality rates of in-hospital patients admitted at the {Kenema} {Government} {Hospital} {Lassa} {Fever} {Ward} between 2016-2018},
	url = {https://www.researchsquare.com/article/rs-7470/v1},
	abstract = {Background: Lassa fever is a zoonotic acute viral illness mainly found in West Africa. The disease is endemic in some parts of West Africa including Sierra Leone, Liberia, Guinea and Nigeria; while other neighboring countries are at high risk of its outbreak since the animal vectors are distributed throughout the region.
Methods: This research adopted a retrospective mixed cohort study that analysed the treatment history containing the sociodemographic and clinical characteristics of 52 laboratory-confirmed LF cases that were admitted to the Kenema Government Hospital Lassa Fever Ward during 2016 to 2018; i.e. during the immediate post Ebola outbreak in Sierra Leone. The patients whose treatment histories were analysed came from either within or outside Kenema district where the Lassa Fever treatment lab is located.
Results: Majority (59.6\%, n = 31/52) of the Lassa fever cases recorded during the period under review were adults; females (65.4\%, n = 34/52). The year 2016 recorded more (40.4\%, n = 21/52) Lassa Fever cases; 2017 (28.8\%, n = 15/52) and 2018(30.8\%, n = 16/52).
Conclusions: We highlighted the significance of Lassa fever prevention and control measures that can target its seasonal epidemics. These measures could include strategies that can reduce human contact with the rodent vectors as well as raise sensitization and awareness about Lassa Fever among local residents especially those residing along the Lassa Fever belt in eastern Sierra Leone.},
	language = {en},
	urldate = {2020-10-20},
	journal = {Journal of Virology and Pathogens},
	author = {Kangbai, Jia Bainga and Kamara, Fatima Kinto and Lahai, Rosaline Chukwu and Gebeh, Fatmata},
