	keywords = {habitat, Lassa fever, ecological zone, geographic distribution, Guinea savanna, montane forest, rainforest, rodent reservoir, seasonal fluctuation},
	pages = {118--127},
	file = {Snapshot:C\:\\Users\\ucbtds4\\Zotero\\storage\\F5F9KSU6\\14772000.2017.html:text/html}
